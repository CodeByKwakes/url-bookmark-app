{
  "npmScope": "phantom-tech-test",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "bookmark-app": {
      "tags": ["types:app", "scope:bookmark"]
    },
    "bookmark-app-e2e": {
      "tags": [],
      "implicitDependencies": ["bookmark-app"]
    }
  }
}
